!!! 5
html(lang='en')
head
    meta(charset='utf-8')
    title Carousel Template · Bootstrap
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(name="description",content=getPreparedDescription())
    meta(name="keywords",content=getPreparedKeywords())
    meta(name="author",content=site.author||'')
    !=getBlock('meta').toHTML()
    !=getBlock('styles').add(["/vendor/twitter-bootstrap/css/bootstrap.css","/vendor/twitter-bootstrap/css/bootstrap-responsive.css","/styles/style.css"]).toHTML()
    //if lt IE 9
      script(src='http://html5shim.googlecode.com/svn/trunk/html5.js')
    //- link(rel='shortcut icon', href='../assets/ico/favicon.ico')
    //- link(rel='apple-touch-icon-precomposed', sizes='144x144', href='../assets/ico/apple-touch-icon-144-precomposed.png')
    //- link(rel='apple-touch-icon-precomposed', sizes='114x114', href='../assets/ico/apple-touch-icon-114-precomposed.png')
    //- link(rel='apple-touch-icon-precomposed', sizes='72x72', href='../assets/ico/apple-touch-icon-72-precomposed.png')
    //- link(rel='apple-touch-icon-precomposed', href='../assets/ico/apple-touch-icon-57-precomposed.png')
  body
    .container.navbar-wrapper
      .navbar.navbar-inverse
        .navbar-inner
          a.btn.btn-navbar(data-toggle='collapse', data-target='.nav-collapse')
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.brand(href='#') 盘石软件
          .nav-collapse.collapse
            ul.nav
              li
                a(href='http://www.pansafe.com') 公司主页
              li
                a(href='https://github.com/pansafe/') Github
              li
                a(href='#members') 研发团队成员
              li
                a(href='#projects') 项目介绍
      block content
      footer
        p.pull-right
          a(href='#') Back to top
        p
          | © 2012 Company, Inc. ·
          a(href='#') Privacy
          | ·
          a(href='#') Terms 
    !=getBlock('scripts').add(["/vendor/jquery.js","/vendor/log.js","/vendor/modernizr.js","/scripts/script.js"]).toHTML()
    script
      !function ($) {
      $(function(){
      // carousel demo
      $('#myCarousel').carousel()
      })
      }(window.jQuery)


      
